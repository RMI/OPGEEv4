      <ProcessGroup name="Acid Wet Gas">
        <ProcessRef name="bar"/>
        <ProcessRef name="baz"/>
        <StreamRef name="s2"/>
      </ProcessGroup>
      <ProcessGroup name="Wet Gas">
        <ProcessRef name="yyy"/>
        <ProcessRef name="zzz"/>
        <StreamRef name="s3"/>
      </ProcessGroup>
      <ProcessGroup name="Dry Gas">
        <ProcessRef name="uuu"/>
        <ProcessRef name="www"/>
        <ProcessRef name="zzz"/>
        <StreamRef name="s4"/>
        <StreamRef name="s5"/>
      </ProcessGroup>
      <ProcessGroup name="Sour Gas Reinjection">
        <ProcessRef name="abc"/>
        <ProcessRef name="def"/>
        <StreamRef name="s6"/>
      </ProcessGroup>
      <ProcessGroup name="CO2-EOR Membrane">
        <ProcessRef name="ghi"/>
        <ProcessRef name="jkl"/>
        <ProcessRef name="mmm"/>
