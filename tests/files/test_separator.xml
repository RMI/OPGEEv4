<?xml version="1.0" encoding="UTF-8"?>
<Model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../opgee/etc/opgee.xsd">

	<Analysis name="test_separator">
		<A name="functional_unit">oil</A>
		<A name="energy_basis">LHV</A>
	  <A name="GWP_years">100</A>
	  <A name="GWP_version">AR5</A>
		<Field name="test"/>
	</Analysis>

	<Field name="test">
		<A name="country">USA</A>
		<Process class="Separation" start="true">
			<Consumes>crude oil</Consumes>
			<Produces>crude oil</Produces>
			<Produces>water</Produces>
			<Produces>natural gas</Produces>
		</Process>

		<Process class="ReservoirWellInterface">
			<Consumes>crude oil</Consumes>
			<Consumes>flooding CO2</Consumes>
			<Produces>crude oil</Produces>
		</Process>

		<Process class="DownholePump">
			<Consumes>crude oil</Consumes>
			<Consumes>lifting gas</Consumes>
			<Produces>crude oil</Produces>
		</Process>

		<Process class="After" desc="Test process 2">
			<Consumes>crude oil</Consumes>
			<Produces>lifting gas</Produces>
		</Process>

		<Stream src="Separation" dst="After" name="oil after separator"/>
		<Stream src="Separation" dst="After" name="water after separator"/>
		<Stream src="Separation" dst="After" name="gas after separator"/>

		<Stream src="Separation" dst="Environment" name="gas fugitives from separator"/>
		<Stream src="DownholePump" dst="Environment" name="gas fugitives from downhole pump"/>

		<Stream src="ReservoirWellInterface" dst="DownholePump" name="gas from reservoir well interface to downhole pump"/>
		<Stream src="ReservoirWellInterface" dst="DownholePump" name="oil from reservoir well interface to downhole pump"/>
		<Stream src="ReservoirWellInterface" dst="DownholePump" name="water from reservoir well interface to downhole pump"/>

		<Stream src="DownholePump" dst="Separation" name="downhole pump to separator"/>

		<Stream src="After" dst="DownholePump" name="lifting gas" impute="False"/>
		<Stream src="After" dst="ReservoirWellInterface" name="flooding CO2" impute="False"/>



	</Field>
</Model>
