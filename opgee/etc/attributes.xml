<?xml version="1.0" encoding="UTF-8"?>
<!-- This file defines attributes of standard OPGEE classes / XML elements, including
     name, short description, unit (if any) and deterministic default values. It is used
     to support parsing the XML model definition and to used by the Smart Default system
     to fill in unspecified attributes. It can also eventually support a GUI, e.g., in
     creating pulldown menus of options for categorical parameters and radio buttons for 
     binaryÂ ones. 
     -->
<Attributes> 
  <Field>
    <Group name="Production_methods">
      <A name="downhole_pump" type="binary" desc="Downhold pump">1</A>
      <A name="water_reinjection" type="binary" desc="Water reinjection">1</A>
      <A name="natural_gas_reinjection" type='binary' desc="Natural gas reinjection">1</A>
      <A name="water_flooding" type="binary" desc="Water flooding">0</A>
      <A name="gas_lifting" type="binary" desc="Gas lifting">0</A>
      <A name="gas_flooding" type="binary" desc="Gas flooding">0</A>
      <A name="steam_flooding" type="binary" desc="Steam flooding">0</A>
      <A name="oil_sands_mine_int" type="binary" desc="Oil sands mine (integrated with upgrader)">0</A>
      <A name="oil_sands_mine_non_int" type="binary" desc="Oil sands mine (non-integrated with upgrader)">0</A>
    </Group>
    <Group name="Field_properties">
      <A name="country" desc="Field location (country)" type='str'>Generic</A>
      <A name="name" desc="Field name" type='str'>Generic</A>
      <A name="age" unit="yr" desc="Field age" type='float'>38</A>
      <A name="depth" unit="ft" desc="Field depth" type='float'>8285</A>
      <A name="oil_prod" unit="bbl/d" desc="Oil production volume" type='float'>2098</A>
      <A name="num_prod_wells" desc="Number of producing wells" type='int'>1158</A>
      <A name="num_water_inj_wells" desc="Number of water injecting wells" type='int'>34436</A>
      <A name="well_diam" unit="in" desc="Well diameter" type='float'>2.8</A>
      <A name="prod_index" unit="bbl/psi(d)" desc="Productivity index">17.0</A>
      <A name="res_press" unit="psia" desc="Average reservoir pressure" type="float">1061.2</A>
      <A name="res_temp" unit="degF" desc="Average reservoir temperature" type="float">158.8</A>
      <A name="offshore" type="binary" desc="Offshore?">0</A>
    </Group>
    <Group name="Fluid_properties">
      <A name="API_grav" desc="API gravity of produced crude" unit="degAPI" type="float">47</A>
      <A name="Gas_comp_N2" unit="mol%" type="float">2.0</A>
      <A name="Gas_comp_CO2" unit="mol%" type="float">6.0</A>
      <A name="Gas_comp_C1" unit="mol%" type="float">84.0</A>
      <A name="Gas_comp_C2" unit="mol%" type="float">4.0</A>
      <A name="Gas_comp_C3" unit="mol%" type="float">2.0</A>
      <A name="Gas_comp_C4" unit="mol%" type="float">1.0</A>
      <A name="Gas_comp_H2S" unit="mol%" type="float">1.0</A>
    </Group>
    <Group name="Production_practices">
      <Options name="flood_gases">
        <Option number="1">Natural gas</Option>
        <Option number="2">Nitrogen (N2)</Option>
        <Option number="3">Carbon dioxide (CO2)</Option>
      </Options>
      <Options name="liquids_unloading">
        <Option number="1">plunger</Option>
        <Option number="2">no plunger</Option>
        <Option number="3">none</Option>
      </Options>
      <Options name="source_CO2">
        <Option number="1">natural subsurface reservoir</Option>
        <Option number="2">anthropogenic</Option>
      </Options>
      <A name="GOR" desc="Gas-to-oil ratio" unit="scf/bbl_oil" type="float">1205.4</A>
      <A name="WOR" desc="Water-to-oil ratio" unit="bbl_water/bbl" type="float">27.7</A>
      <A name="WIR" desc="Water injection ratio" unit="bbl_water/bbl" type="float">17.0</A>
      <A name="GLIR" desc="Gas lifting injection ratio" unit="scf/bbl_liquid" type="float">1500.0</A>
      <A name="GFIR" desc="Gas flooding injection ratio" unit="scf/bbl_oil" type="float">22611.0</A>
      <A name="flood_gas_type" desc="Flood gas" options="flood_gases" type="int">1</A>
      <A name="liquids_unloading" options="liquids_unloading" type="int">3</A>
      <A name="perc_CO2_breakthrough" desc="Fraction of CO2 breaking through to producers" unit="pct" type="float">59</A>
      <A name="source_CO2" desc="" options="source_CO2" type="int">1</A>
      <A name="perc_sequestration_credit" desc="Percentage of sequestration credit assigned to the oilfield" unit="pct" type="float">0</A>
      <A name="SOR" desc="Steam-to-oil ratio" unit="bbl_steam/bbl_oil" type="float">0</A>
      <A name="fraction_elec_onsite" desc="Fraction of required electricity generated onsite" type="float">0.0</A>
      <A name="fraction_remaining_gas_inj" desc="Fraction of remaining gas reinjected" type="float">0.5</A>
      <A name="fraction_water_reinjected" desc="Fraction of water produced that is reinjected" type="float">1.0</A>
      <A name="fraction_steam_cogen" desc="Fraction of steam generation via co-generation" type="float">0.0</A>
      <A name="fraction_steam_solar" desc="Fraction of steam generation via solar thermal" type="float">0.0</A>
    </Group>
    <Group name="Processing_practices">
      <Options name="upgrader_type">
        <Option number="0">None</Option>
        <Option number="1">Delayed coking</Option>
        <Option number="2">Hydroconversion</Option>
        <Option number="3">Combined hydroconversion and fluid coking</Option>
      </Options>
      <Options name="gas_processing_path">
        <Option number="1">None</Option>
        <Option number="2">Minimal: Dehydrator</Option>
        <Option number="3">Acid Gas: Dehydrator + Amine Process</Option>
        <Option number="4">Wet Gas: Dehydrator + Demethanizer</Option>
        <Option number="5">Acid Wet Gas: Dehydrator + Amine Process + Demethanizer</Option>
        <Option number="6">Sour gas reinjection: Dehydration then direct injection of sour CO2-rich stream</Option>
        <Option number="7">CO2-EOR Membrane: Dehydrator + Compressor + Membrane + Amine Process + Demethanizer</Option>
        <Option number="8">CO2-EOR Ryan Holmes: Dehydrator + Ryan Holmes Process (Demethanizer Included)</Option>
      </Options>      
      <A name="heater_treater" type="binary" desc="Heater/treater">0</A>
      <A name="stabilizer_column" type="binary" desc="Stabilizer column">0</A>
      <A name="upgrader_type" desc="Upgrader type" options="upgrader_type" type="int">0</A>
      <A name="gas_processing_path" desc="Associated gas processing path" options="gas_processing_path" type="int">5</A>
      <A name="FOR" unit='scf/bbl' desc="Ratio of flaring to oil production" type="float">141.3</A>
      <A name="VOR" unit='scf/bbl' desc="Ratio of venting to oil production" type="float">5.0</A>
      <A name="fraction_diluent" desc="Volume fraction of diluent in diluted crude" type="float">5.0</A>
    </Group>
    
    <Group name="LandUse_impacts">
      <!-- RP added these; the spreadsheet uses 3 separate binary vars instead -->
      <Options name="ecosystem_C_richness">
        <Option number="1">Low carbon richness (semi-arid grasslands)</Option>        
        <Option number="2">Moderate carbon richness (mixed)</Option>        
        <Option number="3">High carbon richness (forested)</Option>        
      </Options>
      <Options name="field_development_intensity">
        <Option number="1">Low intensity development and low oxidation</Option>        
        <Option number="2">Moderate intensity development and moderate oxidation</Option>        
        <Option number="3">High intensity development and high oxidation</Option>        
      </Options>
      <A name="ecosystem_richness" options="ecosystem_C_richness" desc="Ecosystem carbon richness" type="int">2</A>
      <A name="field_development_intensity" options="field_development_intensity" desc="Field development intensity" type="int">2</A>
    </Group>
    <Group name="Transport_and_storage">
      <A name="Fraction_oil_transport_tanker" desc="Fraction  of oil transported by ocean tanker" type="float">1</A>
      <A name="Fraction_oil_transport_barge" desc="Fraction of oil transported by barge" type="float">0</A>
      <A name="Fraction_oil_transport_pipeline" desc="Fraction of oil transported by pipeline" type="float">1</A>
      <A name="Fraction_oil_transport_rail" desc="Fraction of oil transported by rail" type="float">0</A>
      <A name="Fraction_oil_transport_truck" desc="Fraction of oil transported by truck" type="float">0</A>
      
      <A name="Oil_transport_distance_tanker" unit="mi" desc="Ocean tanker transport distance (one way)" type="float">8000</A>
      <A name="Oil_transport_distance_barge" unit="mi" desc="Barge transport distance (one way)" type="float">500</A>
      <A name="Oil_transport_distance_pipeline" unit="mi" desc="Pipeline transport distance (one way)" type="float">1000</A>
      <A name="Oil_transport_distance_rail" unit="mi" desc="Rail transport distance (one way)" type="float">800</A>
      <A name="Oil_transport_distance_truck" unit="mi" desc="Truck transport distance (one way)" type="float">100</A>
      <A name="ocean_tanker_size" unit="tons" desc="Ocean tanker size, if applicable" type="float">250000</A>
    </Group>
    <Group name="Small_source_emissions">
      <A name="small_sources_adder" unit="gCO2eq/MJ" desc="Small source emissions adder" type="float">0.5</A>
    </Group>
  </Field>
  
  <!-- Exploration and development -->
  <Process name="Exploration">
    <A name="distance_survey" type="float" desc="Distance of travel for survey" unit="mi">10000</A>
    <A name="weight_land_survey" type="float" desc="Weight of land survey vehicle" unit="tons">25</A>
    <A name="weight_ocean_survey" type="float" desc="Weight of ocean survey vehicle" unit="tons">100</A>
    <A name="number_wells_dry" type="int" desc="Number of dry wells drilled per field found">1</A>
    <A name="number_wells_exploratory" type="int" desc="Number of exploratory/scientific wells drilled after field discovery">3</A>
  </Process>
  
  <!-- Drilling and development -->
  <Technology class="Drilling">
    <Options name="rig_efficiency">
      <!-- Allow options to be named "low", "med", "high" rather than requiring a number? -->
      <Option number="1" >Low: Mud pump 60%, Rotary Table 40%</Option>
      <Option number="2" >Med: Mud pump 65%, Rotary Table 45%</Option>
      <Option number="3">High: Mud pump 70%, Rotary Table 50%</Option>
    </Options>
    
    <Options name="well_complexity">
      <Option number="1">Simple = Surface casing plus production casing</Option>
      <Option number="2">Moderate = Surface, intermediate, production</Option>
      <Option number="3">Complex = Surface, Intermediate 1, Intermediate 2, Production</Option>
    </Options>
    
    <Options name="well_size">
      <Option number="1">Small</Option>
      <Option number="2">Medium</Option>
      <Option number="3">Large</Option>
    </Options>
    
    <Options name="field_productivity_category">
      <Option number="1">Low: USA</Option>
      <Option number="2">Med: Global average</Option>
      <Option number="3">High: OPEC</Option>
    </Options>
    
    <Options name="timeframe_land_use">
      <Option number="1">30 years</Option>
      <Option number="2">150 years</Option>
    </Options>
    
    <A name="eta_rig" type="int" options="rig_efficiency" desc="Rig efficiency">2</A>
    <A name="well_complexity" type="int" options="well_complexity" desc="Well complexity">2</A>
    <A name="well_size" desc="Hole size" type="int" options="well_size">2</A>
    
    <A name="fraction_wells_horizontal" type="float" description="Horizontal well?">0</A>    
    <A name="length_lateral" unit="ft" type="float" desc="Length of lateral">5000</A>
    <A name="well_productivity_crude_oil" options="field_productivity_category" type="int">2</A>
    <A name="well_productivity_natural_gas" options="field_productivity_category" type="int">2</A>
        
    <A name="fraction_wells_fractured" desc="Fraction of wells fractured" type="float">0</A>
    <A name="volume_per_well_fractured" desc="Fracturing fluid injection volume" type="float" unit="million gallons">3</A>
    <A name="pressure_gradient_fracturing" desc="Fracturing pressure gradient (rock strength)" unit="psi/ft">0.7</A>
    <A name="timeframe_land_use" desc="Timeframe of land use analysis" options="timeframe_land_use">1</A>
    <A name="flaring_fracturing_flowback" desc="Fracturing flowback flared?" type="binary">0</A>
    <A name="REC_fracturing_flowback" desc="Fracturing flowback captured with reduced emissons completions?" type="binary">1</A>
    <A name="number_well_workovers" desc="Workovers needed over lifetime of well" type="float">4</A>
  </Technology>
  
  <!-- Production -->
  <Technology class="FluidProduction">    
    <A name="baz" type="int" unit="d" desc="xxx">1</A>
  </Technology>
  
  <Technology class="FluidInjection">
    <A name="baz" type="int" unit="d" desc="xxx">1</A>
  </Technology>
  
  <Technology class="BitumenMining">
    <A name="baz" type="int" unit="d" desc="xxx">1</A>
  </Technology>
  
  <Technology class="MaintenanceAndWorkovers">
    <A name="baz" type="int" unit="d" desc="xxx">1</A>
  </Technology>  
  
  <!-- Surface processing-->
  <Technology class="CrudeSeparationAndHandling">
    <A name="baz" type="int" unit="d" desc="xxx">1</A>
  </Technology>  
  
  <Technology class="CrudeStorage">
    <A name="baz" type="int" unit="d" desc="xxx">1</A>
  </Technology>  
  
  <Technology class="BitumentUpgradingOrDilution">
    <A name="baz" type="int" unit="d" desc="xxx">1</A>
  </Technology>  
  
  <Technology class="WasteTreatmentAndDisposal">
    <A name="baz" type="int" unit="d" desc="xxx">1</A>
  </Technology>  
  
  <!-- Transport -->
  <Technology class="CrudeTransport">
    <A name="baz" type="int" unit="d" desc="xxx">1</A>
  </Technology>  
</Attributes>
